{
  "name": "succinctcoin",
  "version": "0.0.1",
  "description": "minimalistic crypto-currency",
  "productName": "succinctcoin",
  "main": "./.webpack/main",
  "keywords": [
    "echowaves",
    "succinct coin",
    "SuccinctCoin",
    "electron",
    "react"
  ],
  "author": "Echowaves <dmitry@echowaves.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest --ci --coverage --reporters=default ",
    "test:ci": "jest --watch",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint . --ext .js,.ts"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "succinctCoin"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "name": "succinctCoin"
          }
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {
            "name": "succinctCoin"
          }
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/renderer/index.html",
                  "js": "./src/renderer/index.js",
                  "name": "main_window",
                  "preload": {
                    "js": "./src/renderer/preload.js"
                  }
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "jest": {
    "testEnvironment": "node"
  },
  "build": {
    "appId": "com.echowaves.SuccinctCoin",
    "files": [],
    "mac": {
      "target": "dmg",
      "icon": "src/static/assets/logo.png"
    },
    "win": {
      "target": "nsis",
      "icon": "src/static/assets/logo.png"
    },
    "linux": {
      "target": "deb",
      "icon": "src/static/assets/logo.png",
      "category": "Development"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-transform-runtime": "7.18.6",
    "@babel/preset-env": "7.18.6",
    "@babel/preset-react": "7.18.6",
    "@electron-forge/cli": "^6.0.0-beta.64",
    "@electron-forge/maker-deb": "^6.0.0-beta.64",
    "@electron-forge/maker-rpm": "^6.0.0-beta.64",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.64",
    "@electron-forge/maker-zip": "^6.0.0-beta.64",
    "@electron-forge/plugin-webpack": "6.0.0-beta.64",
    "@testing-library/react": "13.3.0",
    "@vercel/webpack-asset-relocator-loader": "1.7.3",
    "babel-eslint": "10.1.0",
    "babel-loader": "^8.2.5",
    "concurrently": "7.2.2",
    "cross-env": "7.0.3",
    "css-loader": "6.7.1",
    "electron": "19.0.8",
    "eslint": "8.20.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.5.3",
    "eslint-plugin-jsx-a11y": "6.6.0",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "28.1.2",
    "node-loader": "^2.0.0",
    "style-loader": "^3.3.1"
  },
  "dependencies": {
    "@chainsafe/discv5": "^1.1.1",
    "@chainsafe/libp2p-noise": "^7.0.1",
    "@libp2p/floodsub": "^3.0.2",
    "@libp2p/mdns": "^3.0.0",
    "@libp2p/mplex": "^4.0.0",
    "@libp2p/tcp": "^3.0.2",
    "big.js": "6.2.1",
    "body-parser": "1.20.0",
    "bootstrap": "5.2.0",
    "cors": "2.8.5",
    "electron-fetch": "^1.7.4",
    "electron-is-dev": "2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "express": "4.18.1",
    "flash-store": "1.0.6",
    "ipfs-pubsub-room": "3.0.0",
    "libp2p": "0.37.3",
    "moment": "2.29.4",
    "obj2fs-hoc": "1.3.2",
    "react": "^18.2.0",
    "react-bootstrap": "2.4.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "6.3.0",
    "uuid": "8.3.2"
  }
}
